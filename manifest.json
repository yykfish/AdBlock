{
	"name": "AdBlock",
	"version": "1.0",
	"manifest_version": 2,
	"description": "a simple AdBlock extension for Chrome",
	"permissions": [
    	"http://*/",
    	"http://*/*",
    	"https://*/",
    	"https://*/*"
  	],
	"icons": {
		"16": "icon-16.png",
		"128": "icon-128.png"
	},
	"browser_action": {
		"default_icon": "icon-16.png",
		"default_popup": "popup.html"
	},
	"content_scripts": [
    {
      "matches": ["http://*/","https://*/","http://*/*","https://*/*","http://www.jc88.net/*"],
      "js": ["js/jquery-2.1.4.js", "js/adBlock_script.js"]
    }
  ]
}